      <type>string</type>
      <required>false</required>
      <validations>
        <validation>
          <type>regex</type>
          <configuration>
            <pattern>[0-9]{1,2}(\.[0-9]{1,2})? ?x ?[0-9]{1,2}(\.[0-9]{1,2})?</pattern>
          </configuration>
        </validation>
      </validations>
    </column>
