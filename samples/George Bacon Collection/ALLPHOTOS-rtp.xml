    <column>
      <index>6</index>
      <type>string</type>
      <required>false</required>
      <validations>
        <validation>
          <type>regex</type>
          <configuration>
            <pattern>[1-9][0-9]{0,2}|([1-9]|1[0-9]|2[0-3]),[0-9]{3}(-[A-Z])? </pattern>
          </configuration>
        </validation>
      </validations>
